 <script type='text/javascript' src='http://www.google.com/jsapi'></script>
  <script type='text/javascript'>
    google.load('visualization', '1', {'packages':['annotatedtimeline']});
    google.setOnLoadCallback(drawChart);
    function drawChart() {
      var data = new google.visualization.DataTable();
      data.addColumn('date', 'Date');
      data.addColumn('number', 'PGE');
      data.addColumn('number', 'Solar');
      data.addColumn('number', 'Net');
      data.addColumn('number', 'Water');
      data.addRows([
      <% @dayPowersResult.each do |dp| %>
        
        [new Date(<%= dp.day.year %>, <%= dp.day.month %>, <%= dp.day.day %>), <%= dp.pgeWatts %>, <%= dp.solarWatts %>, <%= dp.pgeWatts + dp.solarWatts %>, <%= dp.waterWatts %> ],

      <% end %>
      ]);

      var chart = new google.visualization.AnnotatedTimeLine(document.getElementById('chart_div'));
      var options = {
        displayAnnotations: false,
        colors: ['red', 'green', 'grey', 'blue']
      };
      chart.draw(data, options );
    }
  </script>

  <div id='chart_div' style='width: 700px; height: 400px;'></div>
