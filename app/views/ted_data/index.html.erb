<h1>Listing ted_data</h1>

<table>
  <tr>
    <th>Mtu</th>
    <th>Mtype</th>
    <th>Cumtime</th>
    <th>Watts</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ted_data.each do |ted_datum| %>
  <tr>
    <td><%= ted_datum.mtu %></td>
    <td><%= ted_datum.mtype %></td>
    <td><%= ted_datum.cumtime %></td>
    <td><%= ted_datum.watts %></td>
    <td><%= link_to 'Show', ted_datum %></td>
    <td><%= link_to 'Edit', edit_ted_datum_path(ted_datum) %></td>
    <td><%= link_to 'Destroy', ted_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ted datum', new_ted_datum_path %>
